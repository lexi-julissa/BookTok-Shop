------------------\n            // Support for OneTrust, in the event the user changes preference mid page load\n            // We are going to classify all Adobe tools under performance but we can customize this later if needed\n            if(_satellite.getVar('Onetrust - Allow - Performance') == true){\n              adobe.optIn.approveAll();\n              adobe.optIn.complete();\n            }\n            //----------------------------------------------\n          \n            if (location.hash === \"#cookie-settings\") {\n                Optanon.ToggleInfoDisplay();\n            }\n\n            if (!('URL' in window)) {\n                return;\n            }\n\n            var url = new window.URL(document.location);\n            var searchParams = url.searchParams;\n\n            if (!searchParams) {\n                return;\n            }\n\n            // Requested by Gabe\n            try {\n                Boolean(searchParams.get('rejectCookies')) && Optanon.RejectAll();\n            } catch (err) {}\n\n            // Requested by Gabe\n            // http://jira.hq.bn-corp.com/browse/BNAPP-905\n            try {\n                Boolean(searchParams.get('acceptCookies')) && Optanon.AllowAll();\n            } catch (err) {}\n        }\n    }\n}());",language:"javascript"}}]},{id:"RL3e4baddeb66144b0a6f502bf4936547a",name:"BOPIS: GEO Detection Success",events:[{modulePath:"core/src/lib/events/directCall.js",settings:{identifier:"onBopisGeoDetectionSuccess"},ruleOrder:50}],conditions:[],actions:[{modulePath:"adobe-analytics/src/lib/actions/setVariables.js",settings:{trackerProperties:{events:[{name:"event134"}]}}},{modulePath:"adobe-analytics/src/lib/actions/sendBeacon.js",settings:{type:"link",linkName:"bopis_geo_detection_success",linkType:"o"}}]},{id:"RL3fa2c7f0a7114779b1f4394112007984",name:"Page: Head: All",events:[{modulePath:"core/src/lib/events/libraryLoaded.js",settings:{},ruleOrder:1}],conditions:[],actions:[{modulePath:"core/src/lib/actions/c